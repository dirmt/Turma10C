package br.com.unixyz.modelo;

public class Endereco {
	private String Logradouro;
	private String numero;
	private String complemento;
	private String bairoo;
	private String cidade;
	private String estato;
	private String cep;
	
	
	
	public Endereco() {
		super();
	}
	
	public Endereco(String logradouro, String numero, String complemento, String bairoo, String cidade, String estato,
			String cep) {
		super();
		Logradouro = logradouro;
		this.numero = numero;
		this.complemento = complemento;
		this.bairoo = bairoo;
		this.cidade = cidade;
		this.estato = estato;
		this.cep = cep;
	}
	
	
	public void setAll(String logradouro, String numero, String complemento, String bairoo, String cidade, String estato,
			String cep) {
		Logradouro = logradouro;
		this.numero = numero;
		this.complemento = complemento;
		this.bairoo = bairoo;
		this.cidade = cidade;
		this.estato = estato;
		this.cep = cep;
	}

	public String getLogradouro() {
		return Logradouro;
	}
	public void setLogradouro(String logradouro) {
		Logradouro = logradouro;
	}
	public String getNumero() {
		return numero;
	}
	public void setNumero(String numero) {
		this.numero = numero;
	}
	public String getComplemento() {
		return complemento;
	}
	public void setComplemento(String complemento) {
		this.complemento = complemento;
	}
	public String getBairoo() {
		return bairoo;
	}
	public void setBairoo(String bairoo) {
		this.bairoo = bairoo;
	}
	public String getCidade() {
		return cidade;
	}
	public void setCidade(String cidade) {
		this.cidade = cidade;
	}
	public String getEstato() {
		return estato;
	}
	public void setEstato(String estato) {
		this.estato = estato;
	}
	public String getCep() {
		return cep;
	}
	public void setCep(String cep) {
		this.cep = cep;
	}
	
	
}
